   <jdbc:property name="user" value="${mariadb.user}"/>
   <jdbc:property name="password" value="${mariadb.pswd}"/>
  </jdbc:readConfig>
  <jdbc:writeConfig>
   <jdbc:dataSourceClassName>${mariadb.class}</jdbc:dataSourceClassName>
   <jdbc:url>${mariadb.url}</jdbc:url>

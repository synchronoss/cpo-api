 <dataConfig xsi:type="ctJdbcConfig" name="driverdriver">
  <metaDescriptorName>jdbcMeta</metaDescriptorName>
  <cpoConfigProcessor>org.synchronoss.cpo.jdbc.config.JdbcCpoConfigProcessor</cpoConfigProcessor>
  <readConfig>
   <driverClassName>${h2.driver}</driverClassName>
   <url>${h2.url}</url>
   <user>${h2.user}</user>
   <password>${h2.pswd}</password>
  </readConfig>
  <writeConfig>
   <driverClassName>${h2.driver}</driverClassName>
   <url>${h2.url}</url>
   <user>${h2.user}</user>
   <password>${h2.pswd}</password>
  </writeConfig>
 </dataConfig>
 <dataConfig xsi:type="ctJdbcConfig" name="classclass">
  <metaDescriptorName>jdbcMeta</metaDescriptorName>
  <cpoConfigProcessor>org.synchronoss.cpo.jdbc.config.JdbcCpoConfigProcessor</cpoConfigProcessor>
  <readConfig>
   <dataSourceClassName>${h2.class}</dataSourceClassName>
   <url>${h2.url}</url>
   <property name="user" value="${h2.user}"/>
   <property name="password" value="${h2.pswd}"/>
  </readConfig>
  <writeConfig>
   <dataSourceClassName>${h2.class}</dataSourceClassName>
   <url>${h2.url}</url>
   <property name="user" value="${h2.user}"/>
   <property name="password" value="${h2.pswd}"/>
  </writeConfig>
 </dataConfig>
 <dataConfig xsi:type="ctJdbcConfig" name="driverclass">
  <metaDescriptorName>jdbcMeta</metaDescriptorName>
  <cpoConfigProcessor>org.synchronoss.cpo.jdbc.config.JdbcCpoConfigProcessor</cpoConfigProcessor>
  <readConfig>
   <driverClassName>${h2.driver}</driverClassName>
   <url>${h2.url}</url>
   <user>${h2.user}</user>
   <password>${h2.pswd}</password>
  </readConfig>
  <writeConfig>
   <dataSourceClassName>${h2.class}</dataSourceClassName>
   <url>${h2.url}</url><property name="user" value="${h2.user}"/>
   <property name="password" value="${h2.pswd}"/>
  </writeConfig>
 </dataConfig>
 <dataConfig xsi:type="ctJdbcConfig" name="classdriver">
  <metaDescriptorName>jdbcMeta</metaDescriptorName>
  <cpoConfigProcessor>org.synchronoss.cpo.jdbc.config.JdbcCpoConfigProcessor</cpoConfigProcessor>
  <readConfig>
   <dataSourceClassName>${h2.class}</dataSourceClassName>
   <url>${h2.url}</url>
    <property name="user" value="${h2.user}"/>
    <property name="password" value="${h2.pswd}"/>
  </readConfig>
  <writeConfig>
   <driverClassName>${h2.driver}</driverClassName>
   <url>${h2.url}</url>
   <user>${h2.user}</user>
   <password>${h2.pswd}</password>
  </writeConfig>
 </dataConfig>
 </cpoConfig>

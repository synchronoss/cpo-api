<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="${h2.type} Jdbc Test Suite" verbose="2">
    <parameter name="db.type" value="${h2.type}"/>
    <parameter name="db.database" value="${h2.database}"/>
    <parameter name="db.port" value="${h2.port}"/>
    <parameter name="db.user" value="${h2.user}"/>
    <parameter name="db.pswd" value="${h2.pswd}"/>
    <parameter name="db.initScript" value="${h2.initScript}"/>
    <parameter name="db.cpoConfig" value="${h2.cpoConfig}"/>
    <listeners>
        <listener class-name="org.synchronoss.cpo.jdbc.JdbcSuiteListener"/>
    </listeners>
    <test name="${h2.type} Jdbc Tests" verbose="2">
        <packages>
            <package name="org.synchronoss.cpo.jdbc.*" />
        </packages>
    </test>
</suite>
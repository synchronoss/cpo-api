   <jdbc:driverClassName>${h2.driver}</jdbc:driverClassName>
   <jdbc:url>${h2.url}</jdbc:url>
   <jdbc:property name="user" value="${h2.user}"/>
   <jdbc:property name="password" value="${h2.pswd}"/>
  </jdbc:readWriteConfig>
 </core:dataConfig>
 <core:dataConfig xsi:type="jdbc:ctJdbcConfig" name="class">
  <core:metaDescriptorName>jdbcMeta</core:metaDescriptorName>
  <core:cpoConfigProcessor>org.synchronoss.cpo.jdbc.config.JdbcCpoConfigProcessor</core:cpoConfigProcessor>
  <jdbc:readWriteConfig>
    <jdbc:dataSourceClassName>${h2.class}</jdbc:dataSourceClassName>
    <jdbc:url>${h2.url}</jdbc:url>
    <jdbc:property name="user" value="${h2.user}"/>
    <jdbc:property name="password" value="${h2.pswd}"/>
  </jdbc:readWriteConfig>
 </core:dataConfig>

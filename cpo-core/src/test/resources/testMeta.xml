<cpoMetaData metaDescriptor="org.synchronoss.cpo.jdbc.meta.JdbcCpoMetaDescriptor" defaultPackageName="" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://www.synchronoss.org/cpo/CpoMeta.xsd">
    <cpoClass name="org.synchronoss.cpo.core.TestObject">
        <cpoAttribute xsi:type="ctAttribute">
            <javaName>idJava</javaName>
            <javaType>java.lang.String</javaType>
            <dataName>idData</dataName>
            <dataType>stringData</dataType>
        </cpoAttribute>
        <cpoAttribute xsi:type="ctAttribute">
            <javaName>nameJava</javaName>
            <javaType>java.lang.String</javaType>
            <dataName>nameData</dataName>
            <dataType>stringData</dataType>
        </cpoAttribute>
        <cpoAttribute xsi:type="ctAttribute">
            <javaName>ageJave</javaName>
            <javaType>int</javaType>
            <dataName>ageData</dataName>
            <dataType>intData</dataType>
        </cpoAttribute>
        <cpoFunctionGroup name="deleteGroup" type="DELETE">
            <cpoFunction name="deleteFunction">
                <expression>delete from test_object where id = ?</expression>
                <cpoArgument xsi:type="ctArgument">
                    <attributeName>id</attributeName>
               </cpoArgument>
            </cpoFunction>
        </cpoFunctionGroup>
        <cpoFunctionGroup name="createGroup" type="CREATE">
            <cpoFunction name="createFunction">
                <expression>insert into test_object (id, name, age) values (?,?,?)</expression>
                <cpoArgument xsi:type="ctArgument">
                    <attributeName>id</attributeName>
                </cpoArgument>
                <cpoArgument xsi:type="ctArgument">
                    <attributeName>name</attributeName>
                </cpoArgument>
                <cpoArgument xsi:type="ctArgument">
                    <attributeName>age</attributeName>
                </cpoArgument>
            </cpoFunction>
        </cpoFunctionGroup>
        <cpoFunctionGroup name="updateGroup" type="UPDATE">
            <cpoFunction name="updateFunction">
                <expression>update test_object set id=?, name=?, age=? where id = ?</expression>
                <cpoArgument xsi:type="ctArgument">
                    <attributeName>id</attributeName>
                </cpoArgument>
                <cpoArgument xsi:type="ctArgument">
                    <attributeName>name</attributeName>
                </cpoArgument>
                <cpoArgument xsi:type="ctArgument">
                    <attributeName>age</attributeName>
                </cpoArgument>
                <cpoArgument xsi:type="ctArgument">
                    <attributeName>id</attributeName>
                </cpoArgument>
            </cpoFunction>
        </cpoFunctionGroup>
        <cpoFunctionGroup name="retrieveGroup" type="RETRIEVE">
            <cpoFunction name="retrieveFunction">
                <expression>select * from test_object where id = ?</expression>
                <cpoArgument xsi:type="ctArgument">
                    <attributeName>id</attributeName>
                </cpoArgument>
            </cpoFunction>
        </cpoFunctionGroup>
    </cpoClass>
</cpoMetaData>
